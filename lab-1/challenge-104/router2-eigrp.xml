<config xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <native xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-native">
    <vrf>
        <definition>
          <name>challenge104-pod16</name>
          <rd>104:16</rd>
          <address-family>
            <ipv4/>
          </address-family>
        </definition>
      </vrf>
    <router>
      <router-eigrp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-eigrp">
        <eigrp>
          <classic-mode>
            <autonomous-system>10416</autonomous-system>
            <address-family>
              <ipv4>
                <af-ipv4-vrf>
                  <vrf>challenge104-pod16</vrf>
                  <autonomous-system>10416</autonomous-system>
                  <network>
                    <address-wildcard>
                      <ipv4-address>10.104.16.0</ipv4-address>
                      <wildcard>0.0.0.255</wildcard>
                    </address-wildcard>
                  </network>
                </af-ipv4-vrf>
              </ipv4>
            </address-family>
          </classic-mode>
        </eigrp>
      </router-eigrp>
    </router>
    <interface>
      <Loopback>
          <name>10416</name>
          <vrf>
            <forwarding>challenge104-pod16</forwarding>
          </vrf>
          <ip>
            <address>
              <primary>
                <address>10.104.16.252</address>
                <mask>255.255.255.255</mask>
              </primary>
            </address>
          </ip>
        </Loopback>
      <GigabitEthernet>
          <name>2.16</name>
          <encapsulation>
            <dot1Q>
              <vlan-id>16</vlan-id>
            </dot1Q>
          </encapsulation>
          <vrf>
            <forwarding>challenge104-pod16</forwarding>
          </vrf>
          <ip>
            <address>
              <primary>
                <address>10.104.16.2</address>
                <mask>255.255.255.252</mask>
              </primary>
            </address>
            <hello-interval-eigrp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-eigrp">
              <hello-interval>
                <eigrp>10416</eigrp>
                <hello-interval-value>7</hello-interval-value>
              </hello-interval>
            </hello-interval-eigrp>
          </ip>
        </GigabitEthernet>
      <GigabitEthernet>
          <name>3.16</name>
          <encapsulation>
            <dot1Q>
              <vlan-id>16</vlan-id>
            </dot1Q>
          </encapsulation>
          <vrf>
            <forwarding>challenge104-pod16</forwarding>
          </vrf>
          <ip>
            <address>
              <primary>
                <address>10.104.16.10</address>
                <mask>255.255.255.252</mask>
              </primary>
            </address>
            <hello-interval-eigrp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XE-eigrp">
              <hello-interval>
                <eigrp>10416</eigrp>
                <hello-interval-value>7</hello-interval-value>
              </hello-interval>
            </hello-interval-eigrp>
          </ip>
        </GigabitEthernet>
    </interface>
  </native>
</config>
